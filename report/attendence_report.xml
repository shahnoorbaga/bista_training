<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
      <template id="bista_training.report_bista_training_batch">

              <t t-call="web.html_container">
                 <t t-foreach="docs" t-as="o">

                  <t t-call="web.external_layout">

                  <div class="page">

                   <h2>Report</h2>
                      <p>
                          <b>Batch: </b> <span t-field="o.name"/>
                          <br/>
                          <b>Student: </b><span t-field="o.trainees"/>
                          <br/>
                          <b>Start Date: </b> <span t-field="o.start_date"/>
                          <br/>
                          <b>End Date: </b> <span t-field="o.end_date"/>




                      </p>


                 </div>

                  </t>
                 </t>
              </t>
      </template>

</data>
</odoo>